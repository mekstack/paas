FLASK_OAUTH_PROVIDERS = ["google", "sample"]

FLASK_GOOGLE_CLIENT_ID = client_id
FLASK_GOOGLE_CLIENT_SECRET = client_secret
FLASK_GOOGLE_AUTHORIZE_PARAMS = {"hd": "test.hd"}
FLASK_GOOGLE_METADATA_URL = https://accounts.google.com/.well-known/openid-configuration

FLASK_SAMPLE_CLIENT_ID = client_id
FLASK_SAMPLE_CLIENT_SECRET = client_secret
FLASK_SAMPLE_METADATA_URL = https://sample.com/.well-known/openid-configuration

FLASK_SECRET_KEY = secret 
FLASK_JWT_SECRET_KEY = secret 
